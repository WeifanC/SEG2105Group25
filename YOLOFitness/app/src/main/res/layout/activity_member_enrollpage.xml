<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".member_enrollpage">

    <Button
        android:id="@+id/bt_unenroll"
        android:layout_width="140dp"
        android:layout_height="53dp"
        android:layout_marginBottom="68dp"
        android:text="unenroll"
        app:layout_constraintBottom_toTopOf="@+id/ltmember"
        app:layout_constraintEnd_toStartOf="@+id/bt_backtomain"
        app:layout_constraintHorizontal_bias="0.646"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bt_enroll"
        app:layout_constraintVertical_bias="0.975" />

    <Button
        android:id="@+id/bt_backtomain"
        android:layout_width="142dp"
        android:layout_height="54dp"
        android:layout_marginBottom="68dp"
        android:text="       Menu       "
        app:layout_constraintBottom_toTopOf="@+id/ltmember"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.773"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bt_enroll"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/bt_enroll"
        android:layout_width="142dp"
        android:layout_height="54dp"
        android:layout_marginBottom="160dp"
        android:text="       Enroll       "
        app:layout_constraintBottom_toTopOf="@+id/ltmember"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ListView
        android:id="@+id/ltmember"
        android:layout_width="303dp"
        android:layout_height="439dp"
        android:layout_marginTop="34dp"
        android:background="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.554"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.606" />

</androidx.constraintlayout.widget.ConstraintLayout>